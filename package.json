{
  "name": "bauhaus-ui",
  "version": "2.0.0",
  "description": "Admin-console for BauhausJS based on React",
  "main": "index.js",
  "bauhausModule": true,
  "scripts": {
    "build": "NODE_ENV=production webpack -p --progress --colors && npm run addes6shim",
    "dev": "webpack --progress --colors --watch",
    "addes6shim": "node addEs6Shim.js",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dustin Robert Hoffner",
  "license": "MIT",
  "dependencies": {
    "basic-auth": "^1.0.3",
    "body-parser": "^1.17.1",
    "fela": "^4.1.0",
    "fela-plugin-fallback-value": "^4.1.0",
    "fela-plugin-friendly-pseudo-class": "^4.1.0",
    "fela-plugin-placeholder-prefixer": "^4.1.0",
    "fela-plugin-prefixer": "^4.1.0",
    "fela-plugin-unit": "^4.1.0",
    "fela-styles-connector": "^1.0.4",
    "fela-stylesheet": "^4.1.0",
    "lodash": "^4.17.4",
    "object-assign": "^4.0.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-fela": "^4.1.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "express": "^4.13.3",
    "history": "^4.6.0",
    "webpack": "^1.14.0"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx"
    ],
    "jsx": {
      "formatJSX": true,
      "attrsOnSameLineAsTag": true,
      "maxAttrsOnTag": 1,
      "firstAttributeOnSameLine": false,
      "spaceInJSXExpressionContainers": " ",
      "alignWithFirstAttribute": true,
      "htmlOptions": {
        "brace_style": "collapse",
        "indent_char": " ",
        "indent_size": 2,
        "max_preserve_newlines": 2,
        "preserve_newlines": true
      }
    }
  }
}
