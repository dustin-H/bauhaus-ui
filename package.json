{
  "name": "bauhaus-ui",
  "version": "0.0.1",
  "description": "bauhaus-ui",
  "main": "index.js",
  "files": [
    "public/"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack -p  --progress --colors",
    "dev": "NODE_ENV=development webpack --progress --colors --watch",
    "minify": "NODE_ENV=production node uglify.js",
    "codecheck": "NODE_ENV=development node uglify.js",
    "codecheckMinify": "NODE_ENV=development node uglify.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dustin Hoffner",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.13.3"
  },
  "devDependencies": {
    "babel-loader": "^5.3.3",
    "bauhaus-ui-module-utils": "latest",
    "body-parser": "^1.15.0",
    "esformatter-jsx": "^4.1.3",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "history": "^2.0.0",
    "http-proxy-middleware": "^0.11.0",
    "lodash": "^4.0.0",
    "material-ui": "^0.14.4",
    "moment": "^2.11.2",
    "object-assign": "^4.0.1",
    "react": "latest",
    "react-dom": "latest",
    "react-look": "^1.0.0-beta6",
    "react-redux": "latest",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "latest",
    "redux-thunk": "latest",
    "scribe-editor": "^2.1.1",
    "scribe-plugin-blockquote-command": "^0.1.0",
    "scribe-plugin-heading-command": "^0.1.0",
    "scribe-plugin-link-prompt-command": "^1.0.0",
    "scribe-plugin-toolbar": "^1.0.0",
    "superagent": "^1.7.0",
    "uglify-js": "^2.5.0",
    "webpack": "^1.12.2"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx"
    ],
    "jsx": {
      "formatJSX": true,
      "attrsOnSameLineAsTag": true,
      "maxAttrsOnTag": 1,
      "firstAttributeOnSameLine": false,
      "spaceInJSXExpressionContainers": " ",
      "alignWithFirstAttribute": true,
      "htmlOptions": {
        "brace_style": "collapse",
        "indent_char": " ",
        "indent_size": 2,
        "max_preserve_newlines": 2,
        "preserve_newlines": true
      }
    }
  }
}
